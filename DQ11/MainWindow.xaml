                        <CheckBox Grid.Row="4" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" IsChecked="{Binding VillageDQ2}"/>
                        <Label Grid.Row="5" Content="DQ3勇者雕像"/>
                        <CheckBox Grid.Row="5" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" IsChecked="{Binding VillageDQ3}"/>
                        <Label Grid.Row="6" Content="DQ4勇者雕像"/>
                        <CheckBox Grid.Row="6" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" IsChecked="{Binding VillageDQ4}"/>
                        <Label Grid.Row="7" Content="DQ5勇者雕像"/>
                        <CheckBox Grid.Row="7" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" IsChecked="{Binding VillageDQ5}"/>
                        <Label Grid.Row="8" Content="DQ6勇者雕像"/>
                        <CheckBox Grid.Row="8" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" IsChecked="{Binding VillageDQ6}"/>
                        <Label Grid.Row="9" Content="DQ7勇者雕像"/>
                        <CheckBox Grid.Row="9" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" IsChecked="{Binding VillageDQ7}"/>
                        <Label Grid.Row="10" Content="DQ8勇者雕像"/>
                        <CheckBox Grid.Row="10" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" IsChecked="{Binding VillageDQ8}"/>
                        <Label Grid.Row="11" Content="DQ9勇者雕像"/>
                        <CheckBox Grid.Row="11" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" IsChecked="{Binding VillageDQ9}"/>
                        <Label Grid.Row="12" Content="DQ10勇者雕像"/>
