                            <ComboBoxItem Content="1阶层"/>
                            <ComboBoxItem Content="2阶层"/>
                            <ComboBoxItem Content="3阶层"/>
                            <ComboBoxItem Content="4阶层"/>
                            <ComboBoxItem Content="5阶层"/>
                            <ComboBoxItem Content="6阶层"/>
                            <ComboBoxItem Content="7阶层"/>
                            <ComboBoxItem Content="8阶层"/>
                            <ComboBoxItem Content="9阶层"/>
                            <ComboBoxItem Content="10阶层"/>
                            <ComboBoxItem Content="全踏破"/>
