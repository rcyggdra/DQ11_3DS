                </Grid>
            </TabItem>
            <TabItem Header="大修炼场">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition Height="12*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Label Grid.Row="1" Content="最快成绩"/>
                    <Label Grid.Row="2" Content="奖励"/>
                    <Label Grid.Column="1" Content="壹之试练"/>
                    <TextBox Grid.Row="1" Grid.Column="1" Text="{Binding TrialFirstScore}"/>
                    <ComboBox Grid.Row="2" Grid.Column="1" SelectedIndex="{Binding TrialFirstStep}">
                        <ComboBoxItem Content="未挑战"/>
                        <ComboBoxItem Content="初次通关"/>
                        <ComboBoxItem Content="16步以内"/>
                        <ComboBoxItem Content="12步以内"/>
                        <ComboBoxItem Content="8步以内"/>
                    </ComboBox>
                    <Label Grid.Column="2" Content="贰之试炼"/>
                    <TextBox Grid.Row="1" Grid.Column="2" Text="{Binding TrialSecondScore}"/>
                    <ComboBox Grid.Row="2" Grid.Column="2" SelectedIndex="{Binding TrialSecondStep}">
                        <ComboBoxItem Content="未挑战"/>
                        <ComboBoxItem Content="初次通关"/>
                        <ComboBoxItem Content="20步以内"/>
                        <ComboBoxItem Content="15步以内"/>
                        <ComboBoxItem Content="10步以内"/>
                    </ComboBox>
                    <Label Grid.Column="3" Content="叁之试炼"/>
                    <TextBox Grid.Row="1" Grid.Column="3" Text="{Binding TrialThirdScore}"/>
                    <ComboBox Grid.Row="2" Grid.Column="3" SelectedIndex="{Binding TrialThirdStep}">
                        <ComboBoxItem Content="未挑战"/>
                        <ComboBoxItem Content="初次通关"/>
                        <ComboBoxItem Content="25步以内"/>
                        <ComboBoxItem Content="20步以内"/>
                        <ComboBoxItem Content="15步以内"/>
                    </ComboBox>
                    <Label Grid.Column="4" Content="肆之试炼"/>
                    <TextBox Grid.Row="1" Grid.Column="4" Text="{Binding TrialFourScore}"/>
                    <ComboBox Grid.Row="2" Grid.Column="4" SelectedIndex="{Binding TrialFourStep}">
                        <ComboBoxItem Content="未挑战"/>
                        <ComboBoxItem Content="初次通关"/>
                        <ComboBoxItem Content="30步以内"/>
                        <ComboBoxItem Content="25步以内"/>
                        <ComboBoxItem Content="20步以内"/>
                    </ComboBox>
                    <Label Grid.Column="5" Content="最终试炼"/>
                    <TextBox Grid.Row="1" Grid.Column="5" Text="{Binding TrialFinalScore}"/>
                    <ComboBox Grid.Row="2" Grid.Column="5" SelectedIndex="{Binding TrialFinalStep}">
                        <ComboBoxItem Content="未挑战"/>
                        <ComboBoxItem Content="初次通关"/>
                        <ComboBoxItem Content="40步以内"/>
                        <ComboBoxItem Content="35步以内"/>
                        <ComboBoxItem Content="30步以内"/>
                    </ComboBox>
                </Grid>
            </TabItem>
            <TabItem Header="不思议锻冶">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="13*"/>
