                        <Label Grid.Row="3" Content="DQ1勇者雕像"/>
                        <CheckBox Grid.Row="3" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" IsChecked="{Binding VillageDQ1}"/>
