                            <ComboBoxItem Content="快"/>
                            <ComboBoxItem Content="普通"/>
                            <ComboBoxItem Content="慢"/>
                        </ComboBox>
                        <ComboBox Grid.Row="6" Grid.Column="1" SelectedIndex="{Binding BGMVolume}">
